{
    "name": "tonylabs/laravel-digikey",
    "description": "Laravel package for DigiKey Product Information API V4 with OAuth2 authentication",
    "type": "library",
    "keywords": [
        "laravel",
        "digikey",
        "api",
        "oauth2",
        "electronics",
        "components"
    ],
    "homepage": "https://github.com/tonylabs/laravel-digikey",
    "license": "MIT",
    "authors": [
        {
            "name": "TONY WANG",
            "email": "tony.wang@tonylabs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": ">=9.0",
        "illuminate/http": ">=9.0",
        "guzzlehttp/guzzle": "^7.8",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "TONYLABS\\DigiKey\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TONYLABS\\DigiKey\\DigiKeyServiceProvider"
            ],
            "aliases": {
                "DigiKey": "TONYLABS\\DigiKey\\Facades\\DigiKey"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
